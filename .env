# Supabase Configuration
VITE_SUPABASE_URL=https://your-project-url.supabase.co
VITE_SUPABASE_ANON_KEY=your-anon-key
SUPABASE_PROJECT_ID=your-project-id
SUPABASE_SERVICE_KEY=your-service-key

# OpenAI Configuration
VITE_OPENAI_API_KEY=your-openai-api-key

# WebSocket Configuration
VITE_WEBSOCKET_URL=https://your-websocket-server.com

# Feature Flags
VITE_ENABLE_AI_ANALYSIS=true
VITE_ENABLE_FRAUD_DETECTION=true

# Monitoring and Logging
VITE_MONITORING_ENDPOINT=https://your-monitoring-service.com/api/events
VITE_LOGGING_ENDPOINT=https://your-logging-service.com/api/logs
VITE_DEBUG_LOGS=false
# Set to false to disable sending logs to external services during development
VITE_ENABLE_EXTERNAL_LOGGING=false

# Security
VITE_ENABLE_CSP=true

# Environment
VITE_ENVIRONMENT=development

# Enable Tempo routes
VITE_TEMPO=true